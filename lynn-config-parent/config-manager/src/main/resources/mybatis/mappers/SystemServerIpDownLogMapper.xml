<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lynn.config.manager.dao.SystemServerIpDownLogDao" >
  <resultMap id="getMap" type="com.lynn.config.api.entity.SystemServerIpDownLog" >
  </resultMap>
  <parameterMap type="com.lynn.config.api.entity.SystemServerIpDownLog" id="putMap" />
  <select id="getSystemServerIpDownLogByCondition" parameterMap="putMap" resultMap="getMap">
    SELECT T.* FROM t_system_server_ip_down_log T WHERE 1 = 1
    <include refid="condition"></include>
    ORDER BY system_name,ip
  </select>
  <sql id="condition">
    <if test="systemName != null and systemName != ''">
      AND T.SYSTEM_NAME = #{systemName}
    </if>
    <if test="ip != null and ip != ''">
      AND T.IP = #{ip}
    </if>
  </sql>
</mapper>